extends jade/common/layout
block header
    link(rel="shortcut icon",href="../favicon.ico")
    link(rel="stylesheet",type="text/css",href="http://hashzhang.com/data/frontend/css/signwall/component.css")
    link(rel="stylesheet", type="text/css",href="http://hashzhang.com/data/frontend/css/statistics/ionicons.css")
    link(rel="stylesheet", type="text/css",href="http://hashzhang.com/data/frontend/css/statistics/_all-skins.css")
    link(rel="stylesheet", href="http://hashzhang.com/data/frontend/lib/AdminLTE/dist/css/AdminLTE.min.css")
block content
    div.container.demo-1(style="width:100%;margin-left: 0px;margin-right: 0px;padding-right: 0px;padding-left: 0px;")
        div.content(style="padding: 0px;")
            div.large-header#large-header
                div.row#head
                    div.col-md-12(style="text-align:center;")
                        h1(style="color: #fff100;font-family: 微软雅黑, 'Microsoft YaHei';font-size: 36px;margin-top:2%;") 2017年会摇一摇
                div#loading(style="margin-top:4%;")
                    img(src="http://hashzhang.com/data/frontend/img/loading/water.gif")
                div(style="position: absolute;top: 15%;left:16%;right:16%;width:68%;padding-bottom:0px;display:none;")#shakeRace
                                ul.products-list.product-list-in-boxs#shake
                div#shakeDiv.row(style="position: absolute;width: 100%;height:100%;margin-left:0px;margin-right:0px;display:none;")
                    div.col-md-12#radiation(style="text-align:center;height:100%;background-image: url('http://hashzhang.com/data/frontend/image/winningStars.png');" class="animated bounceIn")
                        div(style="padding-left:30%;padding-right:30%")
                            div#shakeWinnerDiv.box-body.no-padding.animated.bounceIn.text-center

block footer
block extends
    script(src="http://hashzhang.com/data/frontend/js/signwall/TweenLite.min.js")
    script(src="http://hashzhang.com/data/frontend/js/signwall/EasePack.min.js")
    script(src="http://hashzhang.com/data/frontend/js/signwall/rAF.min.js")
    script(src="http://hashzhang.com/data/frontend/js/signwall/demo-1.min.js")
    script(src="http://hashzhang.com/data/frontend/js/shake.min.js")